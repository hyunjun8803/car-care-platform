{
  "crons": [
    {
      "path": "/api/notifications/reminder",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["icn1"],
  "headers": [
    {
      "source": "/api/shops",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods", 
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        }
      ]
    }
  ]
}